{
  "name": "fbook",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development expo start -c",
    "dev": "cross-env NODE_ENV=development expo start -c",
    "prod": "cross-env NODE_ENV=production expo start -c",
    "test": "cross-env NODE_ENV=test&& expo start -c",
    "build:dev:apk": "cross-env NODE_ENV=development eas build --platform android --profile development --local",
    "build:dev:ios": "cross-env NODE_ENV=development eas build --platform ios --profile development --local",
    "build:prod:aab": "cross-env NODE_ENV=production eas build --platform android --profile production --local",
    "build:prod:apk": "cross-env NODE_ENV=production eas build --platform android --profile productionApk --local",
    "build:prod:ios": "cross-env NODE_ENV=production eas build --platform ios --profile production --local",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/material-top-tabs": "^6.6.10",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "axios": "^1.6.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "expo": "~50.0.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-pager-view": "6.2.3",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-tab-view": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "private": true
}
